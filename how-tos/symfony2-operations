sudo setfacl -R -m u:www-data:rwx -m u:justin:rwx app/cache app/logs app/Resources/java prodimgs testimgs

sudo setfacl -dR -m u:www-data:rwx -m u:justin:rwx app/cache app/logs app/Resources/java prodimgs testimgs


remove cache:

#clear cache
sudo rm -r cache/*

# dump assetic cache
php app/console assetic:dump --env=dev

# clear console cache
php app/console cache:clear --env=dev --no-debug

# symfony2 regenerate assets
php app/console assets:install web

# get routes
php app/console router:debug

