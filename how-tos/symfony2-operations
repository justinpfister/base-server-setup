sudo setfacl -R -m u:www-data:rwx -m u:justin:rwx app/cache app/logs app/Resources/java

sudo setfacl -dR -m u:www-data:rwx -m u:justin:rwx app/cache app/logs app/Resources/java

# dump assetic cache
php app/console assetic:dump --env=dev

# clear console cache
php app/console cache:clear --env=dev --no-debug